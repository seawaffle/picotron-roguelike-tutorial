--[[pod_format="raw",created="2024-10-07 18:35:08",modified="2024-10-07 20:16:37",revision=46]]
function _init()
	player_x = 150
	player_y = 100
end

function _update()
	if btnp(0) then
		player_x -= 16
	elseif btnp(1) then
		player_x += 16
	elseif btnp(2) then
		player_y -= 16
	elseif btnp(3) then
		player_y += 16
	end
end

function _draw()
	cls()
	spr(1, player_x, player_y)
end
